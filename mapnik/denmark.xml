<Map background-color="steelblue" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">

  <Style name="My Style">
    <Rule>
      <PolygonSymbolizer fill="#f2eff9" />
      <LineSymbolizer stroke="rgb(50%,50%,50%)" stroke-width="0.1" />
    </Rule>
  </Style>

  <!-- <Layer name="world" srs="+proj=longlat +lat_0=38.0 +long_0=-77 +lat_1=33 +lat_2=45 +ellps=WGS84 +datum=WGS84 +no_defs"> -->
  <Layer name="denmark" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>My Style</StyleName>
    <Datasource>
      <Parameter name="type">shape</Parameter>
      <Parameter name="file">110m-admin-0-countries/ne_110m_admin_0_countries.shp</Parameter>
    </Datasource>
  </Layer>

</Map>